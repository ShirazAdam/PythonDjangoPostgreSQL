-- Database: PythonDjango

-- DROP DATABASE "PythonDjango";

CREATE DATABASE "PythonDjango"
    WITH 
    OWNER = postgres
    TEMPLATE = template0
    ENCODING = 'UTF8'
    LC_COLLATE = 'en_GB.UTF8'
    LC_CTYPE = 'en_GB.UTF8'
    TABLESPACE = pg_default
    CONNECTION LIMIT = -1;

GRANT TEMPORARY, CONNECT ON DATABASE "PythonDjango" TO PUBLIC;

GRANT CONNECT ON DATABASE "PythonDjango" TO python;